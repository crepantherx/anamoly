{
    "buildCommand": "pip install -r requirements-web.txt",
    "devCommand": "uvicorn app.main:app --host 0.0.0.0 --port 8000",
    "installCommand": "pip install -r requirements-web.txt",
    "builds": [
        {
            "src": "app/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/main.py"
        }
    ]
}